{
  "name": "@vertexvis/stream-api",
  "version": "0.23.0",
  "description": "A websocket client for interacting with Vertex's stream API.",
  "license": "MIT",
  "author": "Vertex Developers <support@vertexvis.com> (https://developer.vertexvis.com)",
  "homepage": "https://github.com/Vertexvis/vertex-web-sdk#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vertexvis/vertex-web-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/Vertexvis/vertex-web-sdk/issues"
  },
  "main": "./dist/bundle.cjs.js",
  "module": "./dist/bundle.esm.js",
  "typings": "./dist/index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    "dist/*",
    "!dist/**/__tests__"
  ],
  "scripts": {
    "clean": "rm -fr ./dist && mkdir ./dist",
    "prebuild": "yarn clean",
    "build": "rollup --config ./rollup.config.js",
    "format": "yarn lint --fix",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx --ignore-path ../../.gitignore .",
    "start": "jest --watch",
    "test": "jest",
    "test:ci": "yarn test:coverage",
    "test:coverage": "yarn test --coverage"
  },
  "dependencies": {
    "@vertexvis/frame-streaming-protos": "^0.13.16"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@vertexvis/eslint-config-vertexvis-typescript": "^0.5.0",
    "@vertexvis/jest-config-vertexvis": "^0.5.4",
    "@vertexvis/utils": "0.23.0",
    "@vertexwebsdk/build": "0.23.0",
    "eslint": "^8.17.0",
    "jest": "^27.5.1",
    "protobufjs": "^6.9.0",
    "rollup": "^2.75.6",
    "ts-jest": "^27.1.4",
    "tslib": "^2.1.0",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "@vertexvis/utils": ">=0.22.0",
    "protobufjs": ">=6.9.0 <7.0.0",
    "tslib": ">=2.1.0"
  }
}
