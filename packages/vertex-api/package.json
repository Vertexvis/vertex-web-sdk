{
  "name": "@vertexvis/vertex-api",
  "version": "0.6.2",
  "description": "Client for interacting with Vertex's RESTful API.",
  "author": "Dan Schultz (https://github.com/danschultz)",
  "license": "MIT",
  "main": "./dist/bundle.cjs.js",
  "module": "./dist/bundle.esm.js",
  "typings": "./dist/index.d.ts",
  "publishConfig": {
    "registry": "https://npm.ops.vertexvis.io/"
  },
  "sideEffects": false,
  "files": [
    "dist/*",
    "!dist/**/__tests__"
  ],
  "scripts": {
    "clean": "rm -fr ./dist && mkdir ./dist",
    "prebuild": "yarn clean",
    "build": "./node_modules/@vertexvis/build-tools/bin/build-web.sh",
    "format": "yarn lint --fix",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx --ignore-path ../../.gitignore .",
    "start": "jest --watch",
    "test": "jest",
    "test:coverage": "yarn test --coverage"
  },
  "dependencies": {
    "@vertexvis/geometry": "0.3.2",
    "@vertexvis/graphics3d": "0.1.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@vertexvis/build-tools": "0.7.2",
    "@vertexvis/eslint-config-vertexvis-typescript": "0.2.1",
    "@vertexvis/jest-config-vertexvis": "0.4.1",
    "@vertexvis/typescript-config-vertexvis": "1.0.1",
    "@vertexvis/network": "0.1.1",
    "@vertexvis/utils": "0.6.1",
    "eslint": "^6.1.0",
    "jest": "^24.9.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.7.4",
    "tslib": "^1.10.0"
  },
  "peerDependencies": {
    "@vertexvis/network": "0.1.1",
    "@vertexvis/utils": "0.6.1"
  }
}
