{
  "name": "@vertexvis/viewer",
  "version": "0.13.1",
  "description": "The Vertex SDK for viewing models.",
  "license": "MIT",
  "author": "Vertex Developers <support@vertexvis.com> (https://developer.vertexvis.com)",
  "homepage": "https://github.com/Vertexvis/vertex-web-sdk#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vertexvis/vertex-web-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/Vertexvis/vertex-web-sdk/issues"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/viewer/viewer.esm.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "files": [
    "dist/",
    "loader/",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rm -fr ./dist && mkdir ./dist",
    "prebuild": "yarn clean",
    "build": "stencil build --docs",
    "postbuild": "./scripts/postbuild.sh",
    "start": "stencil build --dev --watch --serve",
    "format": "yarn lint --fix",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx --ignore-path .gitignore .",
    "test": "stencil test --spec --maxWorkers=50%",
    "test:watch": "stencil test --spec --watchAll --maxWorkers=50%",
    "test:coverage": "stencil test --spec --coverage .spec",
    "generate": "stencil generate",
    "generate:docs": "../../scripts/generate_docs.sh",
    "release:stage": "yarn generate:docs"
  },
  "dependencies": {
    "@improbable-eng/grpc-web": "^0.15.0",
    "@stencil/core": "^2.12.1",
    "@types/classnames": "^2.3.1",
    "@vertexvis/frame-streaming-protos": "^0.6.19",
    "@vertexvis/geometry": "0.13.1",
    "@vertexvis/html-templates": "0.13.1",
    "@vertexvis/scene-tree-protos": "^0.1.12",
    "@vertexvis/scene-view-protos": "^0.1.8",
    "@vertexvis/stream-api": "0.13.1",
    "@vertexvis/utils": "0.13.1",
    "@vertexvis/web-workers": "^0.1.0",
    "camel-case": "^4.1.2",
    "classnames": "^2.3.1",
    "date-fns": "^2.28.0",
    "fast-png": "^6.1.0",
    "google-protobuf": "^3.19.2",
    "jwt-decode": "^3.1.2",
    "param-case": "^3.0.4",
    "protobufjs": "^6.9.0",
    "requestidlecallback-polyfill": "^1.0.2",
    "resize-observer": "^1.0.4",
    "threads": "^1.7.0",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@stencil/react-output-target": "^0.2.1",
    "@types/chance": "^1.1.1",
    "@types/google-protobuf": "^3.7.4",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.6",
    "@vertexvis/eslint-config-vertexvis-typescript": "^0.5.0",
    "@vertexvis/rollup-plugin-vertexvis-copyright": "^0.4.1",
    "@vertexvis/rollup-plugin-web-workers": "^0.1.0",
    "@vertexvis/typescript-config-vertexvis": "1.1.0",
    "@vertexwebsdk/build": "0.13.1",
    "abortcontroller-polyfill": "^1.7.3",
    "chance": "^1.1.8",
    "eslint": "^8.6.0",
    "eslint-plugin-react": "^7.28.0",
    "jest": "^26.6.3",
    "jsonwebtoken": "^8.5.1",
    "rollup": "^2.63.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "ts-jest": "^26.5.2",
    "ts-toolbelt": "^9.6.0",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.4"
  }
}
