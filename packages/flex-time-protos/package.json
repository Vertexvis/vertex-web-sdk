    "postbuild": "tsc"
  },
  "peerDependencies": {
    "google-protobuf": "^3.17.3"
  },
  "devDependencies": {
    "@protobuf-ts/plugin": "^2.0.0-alpha.27",
    "@protobuf-ts/runtime-rpc": "^2.0.0-alpha.27",
    "google-protobuf": "^3.17.3",
    "typescript": "^4.2.2"
  }
}
