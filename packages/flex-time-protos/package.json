{
  "name": "@vertexvis/flex-time-protos",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -fr ./dist && mkdir ./dist && rm -fr ./src && mkdir ./src",
    "prebuild": "yarn clean",
