{
  "name": "@vertexvis/viewer-react",
  "version": "0.6.0",
  "description": "React bindings for the Vertex Viewer SDK.",
  "author": "Dan Schultz <dan.schultz@vertexvis.com>",
  "homepage": "https://github.com/Vertexvis/vertex-web-sdk#readme",
  "bugs": {
    "url": "https://github.com/Vertexvis/vertex-web-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vertexvis/vertex-web-sdk.git"
  },
  "license": "ISC",
  "main": "./dist/bundle.cjs.js",
  "module": "./dist/bundle.esm.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "clean": "rm -fr ./dist && mkdir ./dist",
    "prebuild": "yarn clean",
    "build": "rollup --config ./rollup.config.js",
    "format": "yarn lint --fix",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx --ignore-path ../../.gitignore --ignore-pattern 'src/generated' .",
    "test": "echo \"Error: run tests from root\" && exit 1"
  },
  "dependencies": {
    "@vertexvis/viewer": "^0.6.0"
  },
  "devDependencies": {
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@vertexvis/eslint-config-vertexvis-typescript": "0.2.2",
    "@vertexwebsdk/build": "^0.6.0",
    "eslint": "^6.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rollup": "^1.19.4"
  },
  "peerDependencies": {
    "react": "^16",
    "react-dom": "^16"
  }
}
