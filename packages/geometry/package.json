{
  "name": "@vertexvis/geometry",
  "version": "0.3.2",
  "description": "Library for 2D and 3D geometric types.",
  "author": "Dan Schultz (https://github.com/danschultz)",
  "license": "UNLICENSED",
  "main": "./dist/bundle.cjs.js",
  "module": "./dist/bundle.esm.js",
  "typings": "./dist/index.d.ts",
  "publishConfig": {
    "registry": "https://npm.ops.vertexvis.io/"
  },
  "sideEffects": false,
  "files": [
    "dist/*",
    "!dist/**/__tests__"
  ],
  "scripts": {
    "clean": "rm -fr ./dist && mkdir ./dist",
    "prebuild": "yarn clean",
    "build": "./node_modules/@vertexvis/build-tools/bin/build-web.sh",
    "format": "yarn lint --fix",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx --ignore-path ../../.gitignore .",
    "start": "jest --watch",
    "test": "jest",
    "test:coverage": "yarn test --coverage"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@vertexvis/build-tools": "0.7.1",
    "@vertexvis/eslint-config-vertexvis-typescript": "0.2.0",
    "@vertexvis/jest-config-vertexvis": "0.4.0",
    "@vertexvis/typescript-config-vertexvis": "1.0.0",
    "eslint": "^6.1.0",
    "jest": "^24.9.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.7.4"
  }
}
